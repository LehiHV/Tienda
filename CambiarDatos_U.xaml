<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="Tienda.CambiarDatos_U"
             Title="Cambiar Datos del Usuario">
    <StackLayout Padding="20">
        <Entry Placeholder="Nombre" x:Name="NombreEntry" />
        <Entry Placeholder="Dirección" x:Name="DireccionEntry" />
        <Entry Placeholder="Teléfono" x:Name="TelefonoEntry" />
        <Entry Placeholder="Correo" x:Name="CorreoEntry" />
        <Entry Placeholder="Foto" x:Name="FotoEntry" />
        <Entry Placeholder="Contraseña" x:Name="ContraseñaEntry" />

        <Button Text="Cambiar datos" Clicked="CambiarDatosButton_Clicked" />
    </StackLayout>
</ContentPage>
